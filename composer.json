{
  "type": "library",
  "name": "zero-to-prod/omdb-models",
  "description": "Data Models and Factories for the OMDB API",
  "keywords": [
    "zero-to-prod",
    "Open Movie Database",
    "omdb-models",
    "DataModel"
  ],
  "homepage": "https://github.com/zero-to-prod/omdb-models",
  "support": {
    "email": "dave0016@gmail.com",
    "issues": "https://github.com/zero-to-prod/omdb-models/issues",
    "source": "https://github.com/zero-to-prod/omdb-models",
    "docs": "https://zero-to-prod.github.io/omdb-models",
    "security": "https://github.com/zero-to-prod/omdb-models/blob/main/SECURITY.md"
  },
  "license": "MIT",
  "readme": "./README.md",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com",
      "homepage": "https://davidsmith.blog/",
      "role": "maintainer"
    }
  ],
  "funding": [
    {
      "type": "GitHub",
      "url": "https://github.com/sponsors/zero-to-prod"
    }
  ],
  "scripts": {
    "test-all": [
      "sh test.sh"
    ],
    "docker": [
      "docker compose run --rm "
    ]
  },
  "scripts-descriptions": {
    "test-all": "tests all php versions",
    "docker": "runs a docker container"
  },
  "require": {
    "php": ">=8.1.0",
    "zero-to-prod/data-model": "^81.7",
    "zero-to-prod/transformable": "^71.0.9",
    "zero-to-prod/data-model-helper": "^v81.11.3",
    "zero-to-prod/data-model-factory": "^71.3.2",
    "ext-curl": "*",
    "zero-to-prod/omdb-api": "^1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\OmdbModels\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Factories\\": "factories/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "bin": [
    "bin/omdb-models"
  ],
  "suggest": {
    "zero-to-prod/data-model": "Transform Data to Type-Safe DTOs",
    "zero-to-prod/data-model-helper": "Helpers for a DataModel."
  }
}
